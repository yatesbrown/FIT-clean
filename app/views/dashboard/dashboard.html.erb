<!-- <h1>dashboard</h1>
<%= @user.name%> -->
<div class="main-dashboard-container">

  <div class="top-nav-bar-container">
    <%= link_to image_tag("https://s3.amazonaws.com/fit-pngs/Profile_icon.png", :style => 'height: 8vh; margin-right: 1vh;', id: "profile-icon"), settings_path%>
    <!-- <img src="https://s3.amazonaws.com/fit-pngs/Profile_icon.png" id="profile-icon" alt="Picture of Profile Icon" /> -->
  </div>

  <div class="greeting-container">
    <h3 id="greeting" >Hello, <%= @user.name%></h3>
  </div>
  <div class="last-workouts-container">
    <div class="smaller-inner-div"><div id="last-workouts-h6-container"><h6 id="last-workouts">Last workouts</h6></div></div>
    <div class="bigger-inner-div" id="column-header-container"><div class="column-header"><h6 class="header-content">Date</h6></div><div class="column-header"><h6 class="header-content">Routine</h6></div><div class="column-header"><h6 class="header-content">Time</h6></div></div>
    <div class="bigger-inner-div" id="last-workout-slot-one"><div class="column-header"><h6 class="header-content" id="last-workout-slot-one-col-one">Date</h6></div><div class="column-header"><h6 class="header-content" id="last-workout-slot-one-col-two">Date</h6></div><div class="column-header"><h6 class="header-content" id="last-workout-slot-one-col-three">Date</h6></div></div>
    <div class="bigger-inner-div" id="last-workout-slot-two"><div class="column-header"><h6 class="header-content" id="last-workout-slot-two-col-one">Date</h6></div><div class="column-header"><h6 class="header-content" id="last-workout-slot-two-col-two">Date</h6></div><div class="column-header"><h6 class="header-content" id="last-workout-slot-two-col-three">Date</h6></div></div>
    <div class="bigger-inner-div" id="last-workout-slot-three"><div class="column-header"><h6 class="header-content" id="last-workout-slot-three-col-one">Date</h6></div><div class="column-header"><h6 class="header-content" id="last-workout-slot-three-col-two">Date</h6></div><div class="column-header"><h6 class="header-content" id="last-workout-slot-three-col-three">Date</h6></div></div>
  </div>
  <div class="start-workout-button-container">
    <img id="start-workout-button" src="https://s3.amazonaws.com/fit-pngs/Put+in+Work+button.png" alt="Start workout icon" />
  </div>
</div>
